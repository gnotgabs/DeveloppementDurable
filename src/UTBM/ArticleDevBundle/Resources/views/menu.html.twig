{% macro dynamicMenu(menus) %}
    <div id="menu">
        <ul id="menuDeroulant">
            {% for men in menus %}
            
                {% if men.labelCategory != "Accueil" %}
                    <li><a href="{{ path('article_dev_homepage', {'id': 1 }) }}">{{ men.labelCategory }}</a>
                {% endif %}<ul class="sousMenu">
                            
                {% for smen in men.subCategories %}
                            
                    {% if smen.labelSubCategory != "accueil" %}
                        <li>
                            <a href="{{ path('article_dev_homepage', {'id': smen.id }) }}">
                                {{ smen.labelSubCategory }}
                            </a>
                        </li>
                    {% endif %}
                        
                {% endfor %}
                        
                </ul></li>
                
            {% endfor %}
                
        </ul>
    </div>
{% endmacro %}